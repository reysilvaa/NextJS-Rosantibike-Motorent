    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:next": "next lint --fix --max-warnings=0 || true",
    "clean": "rm -rf .next out",
    "prebuild": "pnpm run clean",
    "analyze": "ANALYZE=true pnpm run build",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "sort-imports": "eslint . --ext .js,.jsx,.ts,.tsx --fix --rule \"simple-import-sort/imports: error\" --rule \"simple-import-sort/exports: error\"",
    "fix:unused": "node scripts/fix-unused-vars.js",
    "fix:all": "pnpm format && pnpm lint:fix && pnpm sort-imports",
    "pm2:start": "NODE_ENV=production pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop rental-frontend",
    "pm2:restart": "pm2 restart rental-frontend",
    "pm2:reload": "pm2 reload rental-frontend",
