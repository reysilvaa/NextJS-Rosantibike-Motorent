  "scripts": {
    "dev": "next dev -p 3001",
    "dev:turbo": "next dev -p 3001 --turbo",
    "dev:inspect": "set NODE_OPTIONS=--inspect && next dev -p 3001 --turbo",
    "build": "next build",
    "build:turbo": "next build --turbo",
    "start": "next start -p 3001",
